    "eslint": "^9.3.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.5.0",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.2.2",
    "unocss": "^0.60.3",
    "unplugin-auto-import": "^0.17.6",
