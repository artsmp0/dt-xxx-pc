  "packageManager": "pnpm@9.5.0",
  "author": "沈孟平 <artsmp@126.com>",
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "dev": "vite --host",
    "build": "vue-tsc && vite build",
