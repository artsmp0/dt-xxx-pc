    "lodash-es": "^4.17.21",
    "lodash-unified": "^1.0.3",
    "naive-ui": "^2.38.2",
    "pinia": "^2.1.7",
    "seemly": "^0.3.8",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.19.1",
    "@gupo-admin/cli": "^0.0.20",
    "@iconify-json/ph": "^1.1.13",
    "@types/node": "^20.12.7",
    "@unocss/eslint-plugin": "^0.60.3",
    "@vitejs/plugin-vue": "^5.0.4",
