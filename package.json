    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-env-types": "^0.1.4",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.0.6"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
